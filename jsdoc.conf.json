{
    "tags": {
        //"allowUnknownTags": true,
        "dictionaries": [
            "jsdoc"
        ]
    },
    "source": {
        "include": [
            "source",
            //"source/tr",
            "source/tr/apps",
            "source/tr/enums",
            "source/tr/abstract.js",
            "source/tr/closure.js",
            "source/tr/composite.js",
            "source/tr/decorator.js",
            "source/tr/factory.js",
            "source/tr/failsafe.js",
            "source/tr/graph.js",
            "source/tr/listener.js",
            "source/tr/observer.js",
            "source/tr/retry.js",
            "source/tr/sleep.js",
            "source/tr/stub.js",
            "source/tr/task.js",
            "source/tr/timeout.js",
            "source/tr/tween.js",
            "source/tr/xhr.js"
        ]
    },
    "plugins": [],
    //"templates": {
        //"cleverLinks": false,
        //"monospaceLinks": false
    //},
    "opts": {
        "template": "node_modules/jaguarjs-jsdoc/", // same as -t templates/default
        "destination": "docs/", // same as -d ./out/
        //"recurse": true, // same as -r
        "readme": "source/README.md" // same as -r path/ro/README
    }
}